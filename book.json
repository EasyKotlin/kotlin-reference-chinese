{
    "language": "zh-hans",
    "plugins": ["regexplace"],
    "pluginsConfig": {
        "regexplace": {
            "substitutes": [
                {
                    "pattern": "<em>([^<]+)</em>\\{: ?\\.keyword\\ ?}",
                    "flags": "g",
                    "substitute": "<em><strong><code>$1</code></strong></em>"
                },
                {
                    "pattern": "\"\/docs\/tutorials\/",
                    "flags": "g",
                    "substitute": "\"http://tanfujun.com/kotlin-web-site-cn/docs/tutorials/"
                }
            ]
        }
    }
}
